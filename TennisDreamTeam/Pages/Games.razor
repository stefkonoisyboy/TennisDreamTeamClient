@page "/games"

<PageTitle>Games</PageTitle>

<MudPaper Square="true" Class="mt-5 mx-3" Elevation="0" Style="background: url(data:image/jpeg;base64,); background-repeat: no-repeat; background-size: cover;">
    <MudGrid Class="pt-5">
        <MudItem Class="d-flex align-center justify-center" xs="12">
            <MudText Style="color: #fff; font-family: 'Kaushan Script', cursive;">
                <strong>TENNIS FANTASY GAME</strong>
            </MudText>
        </MudItem>
    </MudGrid>
    <MudGrid>
        <MudItem Class="d-flex align-center justify-center" xs="12">
            <MudText Style="color: #fff; font-family: 'Kaushan Script', cursive;">
                <strong>BY OMCS</strong>
            </MudText>
        </MudItem>
    </MudGrid>
    <MudGrid Class="pb-5">
        <MudItem Class="d-flex align-center justify-center" xs="12">
            <MudText Style="color: #fff; font-size: 12px; font-family: 'Kaushan Script', cursive;">LEAD YOUR TEAM TO VICTORY</MudText>
        </MudItem>
    </MudGrid>
</MudPaper>

<MudGrid Class="px-3">
    <MudItem Class="d-flex justify-center" xs="12">
        <MudButton OnClick="this.NavigateToTeam" Variant="Variant.Filled" Style="background-color: #02563C; color: #fff; inline-size: 100%;">Play</MudButton>
    </MudItem>
</MudGrid>

<MudGrid Class="px-3 mt-5">
    <MudItem xs="6">
        <MudPaper Square="true" Elevation="0" Style="min-block-size: 140px; background: url(https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQyMB858SjXAnDMi6udhGl5-cVTdrPEnyEETQ&usqp=CAU); background-repeat: no-repeat; background-size: cover;">
            <MudGrid Class="pt-2">
                <MudItem Class="d-flex align-center justify-center" xs="12">
                    <MudText Style="color: #fff; font-family: 'Kaushan Script', cursive;">
                        <strong>QUIZ</strong>
                    </MudText>
                </MudItem>
            </MudGrid>
            <MudGrid Class="pb-2">
                <MudItem Class="d-flex align-center justify-center" xs="12">
                    <MudText Align="Align.Center" Style="color: #fff; font-size: 12px; font-family: 'Kaushan Script', cursive;">TEST YOUR TENNIS KNOWLEDGE</MudText>
                </MudItem>
            </MudGrid>
        </MudPaper>
        <MudButton Variant="Variant.Filled" Style="background-color: #02563C; color: #fff; inline-size: 100%;">Play</MudButton>
    </MudItem>
    <MudItem xs="6">
        <MudPaper Square="true" Elevation="0" Style="min-block-size: 140px; background: url(data:image/jpeg;base64,); background-repeat: no-repeat; background-size: cover;">
            <MudGrid Class="pt-2">
                <MudItem Class="d-flex align-center justify-center" xs="12">
                    <MudText Style="color: #fff; font-family: 'Kaushan Script', cursive;">
                        <strong>BRACKET</strong>
                    </MudText>
                </MudItem>
            </MudGrid>
            <MudGrid Class="pb-2">
                <MudItem Class="d-flex align-center justify-center" xs="12">
                    <MudText Align="Align.Center" Style="color: #fff; font-size: 12px; font-family: 'Kaushan Script', cursive;">BECOME A TOURNAMENT EXPERT</MudText>
                </MudItem>
            </MudGrid>
        </MudPaper>
        <MudButton Variant="Variant.Filled" Style="background-color: #02563C; color: #fff; inline-size: 100%;">Play</MudButton>
    </MudItem>
</MudGrid>

<MudGrid Class="mt-3 px-3">
    <MudItem xs="6">
        <MudText Style="font-family: 'Kaushan Script', cursive;" Align="Align.Start">
            <strong>DASHBOARD</strong>
        </MudText>
    </MudItem>
    <MudItem xs="6">
        <MudText Align="Align.End" Style="color: #555E61; font-family: 'Kaushan Script', cursive;">TOTAL: 2875</MudText>
    </MudItem>
</MudGrid>

<MudGrid Class="mt-6 px-2">
    <MudItem Class="d-flex justify-center" xs="4">
        <div style="background-color: #fff; border: 3px #D76316 solid; border-radius: 50%; block-size: 80px; inline-size: 80px;">
            <MudText Align="Align.Center" Style="color: #D76316; font-size: 25px; line-height: 80px; font-family: 'Kaushan Script', cursive;">
                <strong>765</strong>
            </MudText>
        </div>
    </MudItem>
    <MudItem Class="d-flex justify-center" xs="4">
        <div style="background-color: #fff; border: 3px #507184 solid; border-radius: 50%; block-size: 80px; inline-size: 80px;">
            <MudText Align="Align.Center" Style="color: #507184; font-size: 25px; line-height: 80px; font-family: 'Kaushan Script', cursive;">
                <strong>543</strong>
            </MudText>
        </div>
    </MudItem>
    <MudItem Class="d-flex justify-center" xs="4">
        <div style="background-color: #fff; border: 3px #02563C solid; border-radius: 50%; block-size: 80px; inline-size: 80px;">
            <MudText Align="Align.Center" Style="color: #02563C; font-size: 25px; line-height: 80px; font-family: 'Kaushan Script', cursive;">
                <strong>1567</strong>
            </MudText>
        </div>
    </MudItem>
</MudGrid>

<MudGrid Class="mt-2 px-2">
    <MudItem Class="d-flex justify-center" xs="4">
        <MudText Style="color: #D76316; font-size: 18px; font-family: 'Kaushan Script', cursive;" Align="Align.Center">TEAM</MudText>
    </MudItem>
    <MudItem Class="d-flex justify-center" xs="4">
        <MudText Style="color: #507184; font-size: 18px; font-family: 'Kaushan Script', cursive;" Align="Align.Center">QUIZ</MudText>
    </MudItem>
    <MudItem Class="d-flex justify-center" xs="4">
        <MudText Style="color: #02563C; font-size: 18px; font-family: 'Kaushan Script', cursive;" Align="Align.Center">BRACKET</MudText>
    </MudItem>
</MudGrid>

@code {
    void NavigateToTeam() 
    {
        this.navigationManager.NavigateTo("/team");
    }
}